{% if aliases.user %}<p><b>{{ _('User aliases') }}:</b><br/>
{% for a in aliases.user %}
<b style="color:#888">{{a}} = </b>{{aliases.user[a]}}<br/>
{% endfor %}</p>{% endif %}
{% if aliases.global %}<p><b>{{ _('Global aliases') }}:</b><br/>
{% for a in aliases.global %}
<b style="color:#888">{{a}} = </b>{{aliases.global[a]}}<br/>
{% endfor %}</p>{% endif %}